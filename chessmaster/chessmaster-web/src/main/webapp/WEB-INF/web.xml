<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  	<servlet>
	  <servlet-name>chessPublisherServlet</servlet-name>
	  <servlet-class>ar.com.ucema.ia.chess.web.servlet.ChessPublisherServlet</servlet-class>
